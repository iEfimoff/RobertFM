{
  // Required
  "name": "Live Playlist for smotri.com",
  "version": "2.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["http://smotri.com/*"],
      "js": ["smotri.radio.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "tabs", "http://smotri.com/*"
  ]
}